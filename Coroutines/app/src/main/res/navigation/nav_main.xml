<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_main.xml"
    app:startDestination="@id/hostPostFragment"
    tools:ignore="UnusedNavigation">

    <fragment
        android:id="@+id/hostPostFragment"
        android:name="android.support.design.widget.MenuHostFragment"
        android:label="HostPostFragment"
        app:navGraph="@navigation/nav_post"
        app:navOrder="1"
        tools:layout="@layout/fragment_post" />

    <fragment
        android:id="@+id/albumFragment"
        android:name="com.kantek.coroutines.views.fragments.AlbumFragment"
        android:label="AlbumFragment"
        app:navOrder="2"
        tools:layout="@layout/fragment_album" />

    <fragment
        android:id="@+id/todoFragment"
        android:name="com.kantek.coroutines.views.fragments.TodoFragment"
        android:label="TodoFragment"
        app:navOrder="3"
        tools:layout="@layout/fragment_todo" />

    <fragment
        android:id="@+id/profileFragment"
        android:name="com.kantek.coroutines.views.fragments.ProfileFragment"
        android:label="ProfileFragment"
        app:navOrder="4"
        tools:layout="@layout/fragment_profile" />
</navigation>